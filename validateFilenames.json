{
  "rules": [
    {
      "validation": "camelCase",
      "patterns": ["**/*"]
    },
    {
      "validation": "ignore",
      "patterns": [
        "**/typings/**/*",
        "**/__tests__/**/*",
        "**/__mocks__/**/*",
        "**/__snapshots__/**/*",
        "**/docker-compose.yml",
        "**/Dockerfile*",
        "**/LICENSE",
        "**/README.md"
      ]
    },
    {
      "validation": "PascalCase",
      "patterns": ["src/reporters/*.ts"]
    }
  ]
}
